   "source": [
    "def solution(n):\n",
    "    answer = 0\n",
    "    \n",
    "    li=[]\n",
    "    \n",
    "    while n!=0:\n",
    "        li.append(n % 3)\n",
    "        n //= 3\n",
    "    \n",
    "    for i in range(len(li)):\n",
    "        answer += 3**(len(li)-i-1)*li[i]\n",
    "    \n",
    "    \n",
    "    return answer"
   ]
  },
