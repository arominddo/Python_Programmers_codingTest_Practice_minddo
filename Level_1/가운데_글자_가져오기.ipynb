    "def solution(s):\n",
    "    \n",
    "    tmp = int(len(s)/2)\n",
    "    \n",
    "    if(len(s)%2!=0) :\n",
    "        answer = s[tmp]\n",
    "    else :\n",
    "        answer = s[(tmp-1):(tmp+1)]\n",
    "    \n",
    "    return answer"
   ]
