    "def solution(nums):\n",
    "    answer = 0\n",
    "    \n",
    "    tmp = set(nums)\n",
    "    \n",
    "    if len(tmp) >= (len(nums)/2) :\n",
    "        answer = len(nums)/2\n",
    "    else :\n",
    "        answer = len(tmp)\n",
    "    \n",
    "    return answer"
   ]
