    "def solution(price, money, count):\n",
    "    \n",
    "    Sn = count*(price+price*count)/2\n",
    "    \n",
    "    answer = Sn - money\n",
    "    \n",
    "    if answer<=0 :\n",
    "        answer = 0\n",
    "\n",
    "    return answer"
   ]
