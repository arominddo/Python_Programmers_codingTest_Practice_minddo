    "def solution(A,B):\n",
    "    answer = 0\n",
    "\n",
    "    A.sort()\n",
    "    B.sort(reverse=True)\n",
    "    \n",
    "    \n",
    "    for i in range(len(A)) :\n",
    "        answer += A[i]*B[i]\n",
    "\n",
    "    return answer"
   ]
