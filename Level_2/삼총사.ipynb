    "import itertools\n",
    "\n",
    "def solution(number):\n",
    "    answer = 0\n",
    "    \n",
    "    for a,b,c in itertools.combinations(number,3):\n",
    "        if a+b+c == 0:\n",
    "            answer += 1\n",
    "        \n",
    "    \n",
    "    return answer"
   ]
